; The name of the database
DB: sqliteDb
; Database type
; Currently only supports sqlite3
DB_TYPE: sqlite3



; Tables can be checked for existance
TABLE_EXIST people
TABLE_EXIST icecream


; Tables can be search for column
COLUMN_EXIST people name
COLUMN_EXIST people no
COLUMN_EXIST icecream


; Check for record existance
RECORD_EXISTS people name='Jack' wife='Cassie'
RECORD_EXISTS people name='Jack' AND wife='Cassie'
RECORD_EXISTS people name='Jack' OR name='Cassie'


; Count records 
RECORD_COUNT people 1 name='Jack'


